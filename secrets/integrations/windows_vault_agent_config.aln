# Windows Vault Agent Config - Skeleton
info:
  name: "windows_vault_agent_config"
  version: "0.1.0"
  description: "Vault agent config example for Windows with TLS & encrypted cache enforcement"

structs:
  - name: VaultWindowsConfig
    fields:
      - name: tls_required
        type: boolean
      - name: certificate_pinning
        type: boolean
      - name: cache_encrypted
        type: boolean
      - name: service_account
        type: string

pipeline:
  name: "vault_agent_config_check"
  steps:
    - action: "runtime.mock.vault_agent_check"
      params: { tls_required: true, certificate_pinning: true, cache_encrypted: true }

